{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "department-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/department/DepartmentWebPart.js",
          "manifest": "./src/webparts/department/DepartmentWebPart.manifest.json"
        }
      ]
    },
    "employee-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employee/EmployeeWebPart.js",
          "manifest": "./src/webparts/employee/EmployeeWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DepartmentWebPartStrings": "lib/webparts/department/loc/{locale}.js",
    "EmployeeWebPartStrings": "lib/webparts/employee/loc/{locale}.js"
  }
}
